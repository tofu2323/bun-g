    "@sveltejs/kit": "^1.20.4",
    "@types/cookie": "^0.5.1",
    "prettier-plugin-svelte": "^2.4.0",
    "prettier": "^2.5.1",
    "svelte": "^4.0.5",
    "svelte-adapter-bun": "^0.5.0",
    "svelte-check": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "husky": "^7.0.4"
  },
  "type": "module"
}
